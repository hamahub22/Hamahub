-- ESP Functionality (Enhanced for Wall Visibility and Highlighting)
local espConnections = {};

local function createESP(player)
    if not player or not player.Character then return end
    local character = player.Character
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if not humanoidRootPart then return end

    local box = Instance.new("BoxHandleAdornment")
    box.Name = "ESPBox"
    box.Adornee = humanoidRootPart
    box.AlwaysOnTop = true  -- Makes it visible through walls
    box.ZIndex = 10  -- Higher priority for visibility
    box.Size = humanoidRootPart.Size + Vector3.new(1, 3, 1)  -- Larger for better highlighting
    box.Transparency = 0.3  -- More opaque for glow effect
    box.Color3 = Color3.fromRGB(255, 0, 0)  -- Bright red highlight
    box.Parent = humanoidRootPart
    box.Visible = espEnabled
    print("ESP highlight created for " .. player.Name .. " (visible through walls)");

    -- Clean up old connections if they exist
    if espConnections[player] then
        espConnections[player]:Disconnect()
    end
    espConnections[player] = player.Character.AncestryChanged:Connect(function()
        removeESP(player)
        if espEnabled then
            createESP(player)
        end
    end)
end

local function removeESP(player)
    if not player then return end
    if player.Character then
      local humanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
      if humanoidRootPart then
        local espBox = humanoidRootPart:FindFirstChild("ESPBox")
        if espBox then
          espBox:Destroy()
        end
      end
    end

    if espConnections[player] then
        espConnections[player]:Disconnect()
        espConnections[player] = nil
        print("ESP highlight removed for " .. player.Name);
    end
end

v13.PlayerAdded:Connect(function(player)
    player.CharacterAdded:Connect(function(character)
        if espEnabled then
            createESP(player)
        end
    end)
end)

v13.PlayerRemoving:Connect(removeESP)

-- NoClip Functionality (With RunService Loop for Persistence)
local function applyNoClip(character)
    if not character then return end
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            part.CanCollide = not noClipEnabled
        end
    end
end

v14.CharacterAdded:Connect(function(character)
    wait(1)
    applyNoClip(character)
end)

-- Maintain NoClip with RunService (non-blocking)
v16.RenderStepped:Connect(function()
    if noClipEnabled then
        applyNoClip(v14.Character)
    end
end)

-- Speed Functionality (Super Fast - Set to 100)
local originalWalkSpeed = 16
local newWalkSpeed = 100  -- Super duper fast!

local function applySpeed()
    local character = v14.Character
    if not character then return end
    local humanoid = character:FindFirstChild("Humanoid")
    if not humanoid then return end
    humanoid.WalkSpeed = speedEnabled and newWalkSpeed or originalWalkSpeed
end

v14.CharacterAdded:Connect(function(character)
    wait(1)
    applySpeed()
end)

-- Maintain Speed with RunService (non-blocking)
v16.RenderStepped:Connect(function()
    if speedEnabled then
        applySpeed()
    end
end)
  
    end
}); v6:CreateButton({
    Name = "Faqat dinar w hama",
    Callback = function()
        --// Services //--
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

--// Variables //--
local localPlayer = Players.LocalPlayer
local character = localPlayer.Character or localPlayer.CharacterAdded:Wait()
local humanoid = character:WaitForChild("Humanoid")
local rootPart = character:WaitForChild("HumanoidRootPart")

--// Configuration //--
local sinkSpeed = 0.1 -- How fast the body sinks
local sinkDistance = -5 -- How far the body sinks below the ground

--// Functions //--
local function sinkBody()
    for _, part in pairs(character:GetChildren()) do
        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
            part.Transparency = 0.5 --Make body transparent initially
            part.Anchored = true -- Anchor the part to prevent it from falling due to physics
            local startPosition = part.Position
            local targetPosition = startPosition + Vector3.new(0, sinkDistance, 0)
			
			local tweenInfo = TweenInfo.new(
				math.abs(sinkDistance / sinkSpeed),  -- Duration based on distance and speed
				Enum.EasingStyle.Linear,   -- Easing style (e.g., Linear, Quad, etc.)
				Enum.EasingDirection.Out,  -- Easing direction (e.g., In, Out, InOut)
				0,          -- Repeat count (0 for no repeat)
				false,       -- Reverses (boolean)
				0           -- DelayTime (number)
			)

			local tween = game:GetService("TweenService"):Create(part, tweenInfo, {Position = targetPosition})
            tween:Play()
        end
    end
end

local function resetBody()
	for _, part in pairs(character:GetChildren()) do
        if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
            part.Transparency = 0
            part.Anchored = false
			-- Reset the position of the part.  You might need to store the original position
			-- to properly reset it here, depending on your needs.  For this example, we assume
			-- the parts are only sunk once and not moved otherwise.
			-- If you moved the character around while sunk, resetting the position becomes more complex.
			part.Position = part.Position - Vector3.new(0, sinkDistance, 0)

        end
    end
end


--// Main //--
sinkBody()

-- Example to reset the body after some time
--wait(10)
--resetBody() -- This is a revised Lua script for Roblox, intended for educational purposes only.
-- It creates a marker system where clicking/tapping (left mouse button or touch) in the game world marks that position,
-- with GUI buttons to toggle teleport (on/off) and delete the marker.
-- Only one marker can exist at a time; you must delete it before placing a new one.
-- Teleport happens once when toggling on (if a marker exists).
-- The GUI is now movable by dragging (works on PC and mobile).

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
local markedPosition = nil
local isOn = false

-- Get the player's mouse (for PC)
local mouse = player:GetMouse()

-- Create GUI
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = player.PlayerGui

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 200, 0, 100)
frame.Position = UDim2.new(0.8, 0, 0.8, 0)  -- Bottom-right corner
frame.BackgroundColor3 = Color3.new(0.5, 0.5, 0.5)
frame.BackgroundTransparency = 0.5
frame.Parent = screenGui

local onButton = Instance.new("TextButton")
onButton.Size = UDim2.new(0.4, 0, 0.5, 0)
onButton.Position = UDim2.new(0, 0, 0, 0)
onButton.Text = "Off"
onButton.BackgroundColor3 = Color3.new(0.8, 0.8, 0.8)
onButton.Parent = frame

local deleteButton = Instance.new("TextButton")
deleteButton.Size = UDim2.new(0.4, 0, 0.5, 0)
deleteButton.Position = UDim2.new(0.6, 0, 0, 0)
deleteButton.Text = "Delete"
deleteButton.BackgroundColor3 = Color3.new(0.8, 0.8, 0.8)
deleteButton.Parent = frame

-- Dragging functionality for mobile and PC
local dragging = false
local dragStart = nil
local startPos = nil

frame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragging = true
        dragStart = input.Position
        startPos = frame.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragging = false
            end
        end)
    end
end)

frame.InputChanged:Connect(function(input)
    if dragging and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
        local delta = input.Position - dragStart
        frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

-- Function to mark position on mouse click/tap (only if no marker exists)
mouse.Button1Down:Connect(function()
    if markedPosition == nil and mouse.Target then
        markedPosition = mouse.Hit.Position  -- Mark the clicked position
        print("Marked at: " .. tostring(markedPosition))
    end
end)

-- On/Off button: Toggle teleport to marked position (if exists)
onButton.MouseButton1Click:Connect(function()
    if markedPosition then
        isOn = not isOn
        onButton.Text = isOn and "On" or "Off"
        if isOn then
            humanoidRootPart.CFrame = CFrame.new(markedPosition)  -- Teleport to marker
            print("Teleported to marker")
        end
    else
        print("No marker set")
    end
end)

-- Delete button: Remove marker and reset
deleteButton.MouseButton1Click:Connect(function()
    markedPosition = nil
    isOn = false
    onButton.Text = "Off"
    print("Marker deleted")
end)

    end
