-- Draggable
do
    local function update(input)
        local delta = input.Position - State.DragStart
        Main.Position = UDim2.new(State.StartPos.X.Scale, State.StartPos.X.Offset + delta.X, State.StartPos.Y.Scale, State.StartPos.Y.Offset + delta.Y)
    end
    TitleBar.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            State.Dragging = true
            State.DragStart = input.Position
            State.StartPos = Main.Position
            State.DragInput = input
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    State.Dragging = false
                end
            end)
        end
    end)
    TitleBar.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            State.DragInput = input
        end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if State.Dragging and input == State.DragInput then
            update(input)
        end
    end)
end

-- Utility
local function setToggleVisual(on)
    if on then
        ToggleBtn.Text = "ON"
        ToggleBtn.BackgroundColor3 = Color3.fromRGB(40, 130, 60)
    else
        ToggleBtn.Text = "OFF"
        ToggleBtn.BackgroundColor3 = Color3.fromRGB(130, 40, 40)
    end
end

local function setBoostVisual(on)
    if on then
        BoostBtn.Text = "ON"
        BoostBtn.BackgroundColor3 = Color3.fromRGB(40, 130, 60)
    else
        BoostBtn.Text = "OFF"
        BoostBtn.BackgroundColor3 = Color3.fromRGB(130, 40, 40)
    end
end

local function clamp(n, min, max)
    if n < min then return min end
    if n > max then return max end
    return n
end

local function updateSpeedLabel()
    SpeedLabel.Text = "Speed: " .. math.floor(State.Speed + 0.5)
end

local function setSliderFromSpeed()
    local trackAbs = SliderTrack.AbsoluteSize.X
    if trackAbs <= 0 then return end
    local alpha = (State.Speed - State.MinSpeed) / (State.MaxSpeed - State.MinSpeed)
    local x = math.floor(alpha * trackAbs + 0.5)
    SliderFill.Size = UDim2.new(0, x, 1, 0)
    SliderKnob.Position = UDim2.new(0, SliderTrack.AbsolutePosition.X and (SliderTrack.AbsolutePosition.X) or 0, 0, 0) -- placeholder to avoid race
    SliderKnob.Position = UDim2.new(0, SliderTrack.AbsolutePosition.X and (x + SliderTrack.AbsolutePosition.X) or x, 0, 23) -- will be corrected below
end

local function updateSliderVisual()
    local trackWidth = SliderTrack.AbsoluteSize.X
    local alpha = (State.Speed - State.MinSpeed) / (State.MaxSpeed - State.MinSpeed)
    local px = math.floor(alpha * trackWidth + 0.5)
    SliderFill.Size = UDim2.new(0, px, 1, 0)
    local knobX = SliderTrack.AbsolutePosition.X + px - (SliderKnob.AbsoluteSize.X / 2)
    local relX = px - (SliderKnob.AbsoluteSize.X / 2)
    SliderKnob.Position = UDim2.new(0, clamp(relX, -7, trackWidth - 7), 0, 23)
end

local function setSpeed(n)
    State.Speed = clamp(n, State.MinSpeed, State.MaxSpeed)
    updateSpeedLabel()
    updateSliderVisual()
end

-- Slider input
do
    local draggingSlider = false
    local function setFromX(x)
        local base = SliderTrack.AbsolutePosition.X
        local width = SliderTrack.AbsoluteSize.X
        local rel = clamp(x - base, 0, width)
        local alpha = 0
        if width > 0 then
            alpha = rel / width
        end
        local value = State.MinSpeed + alpha * (State.MaxSpeed - State.MinSpeed)
        setSpeed(value)
    end
    SliderTrack.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            draggingSlider = true
            setFromX(input.Position.X)
        end
    end)
    SliderTrack.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            draggingSlider = false
        end
    end)
    SliderKnob.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            draggingSlider = true
        end
    end)
    SliderKnob.InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            draggingSlider = false
        end
    end)
    UserInputService.InputChanged:Connect(function(input)
        if draggingSlider and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
            setFromX(input.Position.X)
        end
    end)
end

SpeedBox.FocusLost:Connect(function(enterPressed)
    local txt = SpeedBox.Text:gsub("[^%d%.]", "")
    local num = tonumber(txt)
    if num then
        setSpeed(num)
    end
end)

BoostBtn.MouseButton1Click:Connect(function()
    State.BoostEnabled = not State.BoostEnabled
    setBoostVisual(State.BoostEnabled)
end)

MultBox.FocusLost:Connect(function(enterPressed)
    local txt = MultBox.Text:gsub("[^%d%.]", "")
    local num = tonumber(txt)
    if num and num > 0 then
        State.BoostMultiplier = clamp(num, 0.1, 50)
        MultBox.Text = tostring(State.BoostMultiplier)
    else
        MultBox.Text = tostring(State.BoostMultiplier)
    end
end)

-- Movement engine
local RaycastParamsCache = RaycastParams.new()
RaycastParamsCache.FilterType = Enum.RaycastFilterType.Exclude
RaycastParamsCache.IgnoreWater = false

local function getIgnoreList(char)
    local list = {}
    if char then
        table.insert(list, char)
    end
    return list
end

local function safeSetWalkspeed(hum, ws)
    if hum and hum.Parent then
        hum.WalkSpeed = ws
    end
end

local function computeMoveDirection(hum)
    if hum and hum.Parent then
        local dir = hum.MoveDirection
        return Vector3.new(dir.X, 0, dir.Z)
    end
    return Vector3.zero
end

local function faceVector(hrp, look)
    local pos = hrp.Position
    local dir = look.Magnitude > 0 and look.Unit or State.LastLook
    State.LastLook = dir
    return CFrame.lookAt(pos, pos + Vector3.new(dir.X, 0, dir.Z))
end

local function moveTP(dt)
    local char, hum, hrp = getCharacter()
    if not hum or not hrp then return end
    if not State.Enabled then return end

    local effectiveSpeed = State.Speed * (State.BoostEnabled and State.BoostMultiplier or 1)
    local dir = computeMoveDirection(hum)
    if dir.Magnitude <= 0 then
        return
    end

    local stepDist = effectiveSpeed * dt
    if stepDist <= 0 then return end

    local startPos = hrp.Position
    if not State.LastGoodY then State.LastGoodY = startPos.Y end

    local planarDir = dir.Unit
    local desired = startPos + planarDir * stepDist

    RaycastParamsCache.FilterDescendantsInstances = getIgnoreList(char)
    local hit = Workspace:Raycast(startPos, (desired - startPos), RaycastParamsCache)
    if hit then
        -- DON'T inherit the hit Y (prevents snapping to ceilings)
        -- Move desired to the hit XY but keep current Y, then back off a bit along planarDir
        desired = Vector3.new(hit.Position.X, startPos.Y, hit.Position.Z) - planarDir * 1
    end

    -- robust ground search that ignores underside/overhang hits
    local function findGroundBelow(point)
        local start = point + Vector3.new(0, 100, 0)
        local attempts = 0
        while attempts < 8 do
            RaycastParamsCache.FilterDescendantsInstances = getIgnoreList(char)
            local res = Workspace:Raycast(start, Vector3.new(0, -400, 0), RaycastParamsCache)
            if not res then
                return nil
            end
            local normal = res.Normal or Vector3.new(0, 1, 0)
            -- require the surface normal to point mostly upward to be considered ground
            if normal.Y >= 0.6 then
                return res.Position
            else
                -- Hit underside or steep wall  continue searching below this hit
                start = res.Position - Vector3.new(0, 1, 0)
            end
            attempts = attempts + 1
        end
        return nil
    end

    local groundPos = findGroundBelow(desired)
    local y = desired.Y
    if groundPos then
        local hip = hum.HipHeight
        y = groundPos.Y + hip
        State.LastGoodY = y
    else
        y = State.LastGoodY or desired.Y
    end

    -- NEW SAFETY: don't allow teleporting upward more than a tiny step
    local maxUp = 2 -- max allowed upward change in studs
    if y > startPos.Y + maxUp then
        y = math.min(startPos.Y + maxUp, State.LastGoodY or startPos.Y)
    end

    local newPos = Vector3.new(desired.X, y, desired.Z)
    local cf = CFrame.lookAt(newPos, newPos + Vector3.new(planarDir.X, 0, planarDir.Z))
    hrp.CFrame = cf
    hrp.Velocity = Vector3.zero
    hum:Move(Vector3.zero, true)
end

local function startEngine()
    if State.HeartbeatConn then State.HeartbeatConn:Disconnect() end
    State.HeartbeatConn = RunService.Heartbeat:Connect(function(dt)
        local ok, err = pcall(function()
            moveTP(dt)
        end)
        if not ok then
            -- minimal safety delay
            task.wait(0.02)
        end
    end)
    task.spawn(function()
        while State.Enabled do
            local _, hum, _ = getCharacter()
            if hum and hum.Parent then
                if tick() - State.LastSetWalk > 0.25 then
                    safeSetWalkspeed(hum, 0)
                    State.LastSetWalk = tick()
                end
            end
            task.wait(0.1)
        end
    end)
end

local function stopEngine()
    if State.HeartbeatConn then
        State.HeartbeatConn:Disconnect()
        State.HeartbeatConn = nil
    end
    local char, hum, _ = getCharacter()
    if hum and hum.Parent then
        safeSetWalkspeed(hum, State.SavedWalkSpeed or 16)
    end
end

local function enableTP(on)
    local char, hum, _ = getCharacter()
    if hum and hum.Parent then
        if on then
            State.SavedWalkSpeed = hum.WalkSpeed
            safeSetWalkspeed(hum, 0)
            startEngine()
        else
            stopEngine()
        end
    end
    setToggleVisual(on)
end

ToggleBtn.MouseButton1Click:Connect(function()
    State.Enabled = not State.Enabled
    enableTP(State.Enabled)
end)

-- Character lifecycle
LP.CharacterAdded:Connect(function(char)
    task.wait(0.5)
    State.LastGoodY = nil
    if State.Enabled then
        enableTP(true)
    end
end)

-- Initialize
setToggleVisual(State.Enabled)
setBoostVisual(State.BoostEnabled)
updateSpeedLabel()
task.wait(0.05)
updateSliderVisual()
    end
});
    end
});
