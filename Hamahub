local v54 = {};
local function GetActiveTarget()
    for _, player in pairs(v13:GetPlayers()) do
        if v54[player.Name] then
            return player;
        end
    end
    return nil;
end

local v56 = false; -- Infinite Jump state
local espEnabled = false; -- ESP state
local noClipEnabled = false; -- NoClip state
local speedEnabled = false; -- Speed state

-- Define toggle functions first for reliability
local function toggleESP()
    print("ESP toggle called, enabled: " .. tostring(espEnabled));
    for _, player in pairs(v13:GetPlayers()) do
        if player ~= v14 then
            local character = player.Character or player.CharacterAdded:Wait()[1]
            if espEnabled then
                createESP(player);
            else
                removeESP(player);
            end
        end
    end
end

local function toggleNoClip()
    print("NoClip toggle called, enabled: " .. tostring(noClipEnabled));
    local character = v14.Character;
    if character then
        applyNoClip(character);
    end
end

local function toggleSpeed()
    print("Speed toggle called, enabled: " .. tostring(speedEnabled));
    applySpeed();
end

