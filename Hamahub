ToggleButton.Size = UDim2.new(1, 0, 0.25, 0)
ToggleButton.Position = UDim2.new(0, 0, 0, 0)
ToggleButton.Text = "Fly: OFF"
ToggleButton.TextColor3 = Color3.new(1, 1, 1)

local SpeedUpButton = Instance.new("TextButton")
SpeedUpButton.Name = "SpeedUpButton"
SpeedUpButton.Parent = MainFrame
SpeedUpButton.BackgroundColor3 = Color3.new(0.4, 0.4, 0.4)
SpeedUpButton.Size = UDim2.new(0.45, 0, 0.25, 0)
SpeedUpButton.Position = UDim2.new(0.025, 0, 0.3, 0)
SpeedUpButton.Text = "+"
SpeedUpButton.TextColor3 = Color3.new(1, 1, 1)

local SpeedDownButton = Instance.new("TextButton")
SpeedDownButton.Name = "SpeedDownButton"
SpeedDownButton.Parent = MainFrame
SpeedDownButton.BackgroundColor3 = Color3.new(0.4, 0.4, 0.4)
SpeedDownButton.Size = UDim2.new(0.45, 0, 0.25, 0)
SpeedDownButton.Position = UDim2.new(0.525, 0, 0.3, 0)
SpeedDownButton.Text = "-"
SpeedDownButton.TextColor3 = Color3.new(1, 1, 1)

local SpeedLabel = Instance.new("TextLabel")
SpeedLabel.Name = "SpeedLabel"
SpeedLabel.Parent = MainFrame
SpeedLabel.BackgroundTransparency = 1
SpeedLabel.Size = UDim2.new(1, 0, 0.2, 0)
SpeedLabel.Position = UDim2.new(0, 0, 0.55, 0)
SpeedLabel.Text = "Speed: 50"
SpeedLabel.TextColor3 = Color3.new(1, 1, 1)
SpeedLabel.TextScaled = true
SpeedLabel.TextXAlignment = Enum.TextXAlignment.Center

--// Variables
local flying = false
local speed = 50
local runService = game:GetService("RunService")
local camera = workspace.CurrentCamera
local humanoid, rootPart

-- Helper: Wait for character
local function setupCharacter()
	local char = player.Character or player.CharacterAdded:Wait()
	humanoid = char:WaitForChild("Humanoid")
	rootPart = char:WaitForChild("HumanoidRootPart")
end

setupCharacter()
player.CharacterAdded:Connect(setupCharacter)

--// Functions
local function updateSpeedLabel()
	SpeedLabel.Text = "Speed: " .. speed
end

local function setFly(state)
	flying = state
	if humanoid then
		humanoid:ChangeState(Enum.HumanoidStateType.Physics)
	end
end

--// Fly movement (joystick controlled)
runService.Heartbeat:Connect(function()
	if flying and humanoid and rootPart then
		local moveDir = humanoid.MoveDirection
		local camLook = camera.CFrame.LookVector

		-- Control height with camera tilt
		local tiltY = camLook.Y -- if camera looks up or down
		local flyDirection = Vector3.new(moveDir.X, tiltY, moveDir.Z)

		if flyDirection.Magnitude > 0 then
			flyDirection = flyDirection.Unit * speed
		end

		rootPart.Velocity = flyDirection
	end
end)

--// GUI Buttons
ToggleButton.MouseButton1Click:Connect(function()
	flying = not flying
	if flying then
		ToggleButton.Text = "Fly: ON"
		setFly(true)
	else
		ToggleButton.Text = "Fly: OFF"
		setFly(false)
		if rootPart then
			rootPart.Velocity = Vector3.zero
		end
	end
end)

SpeedUpButton.MouseButton1Click:Connect(function()
	speed = speed + 10
	updateSpeedLabel()
end)

SpeedDownButton.MouseButton1Click:Connect(function()
	speed = math.max(10, speed - 10)
	updateSpeedLabel()
end)

--// Initial
updateSpeedLabel()
setFly(false)

    end
});  v2:CreateButton({
    Name = "bang script",
    Callback = function()
        -- LocalScript: Mobile Player Teleport & Attack GUI (Delta Executor friendly)
-- Notes:
--  - Intended for client-side/local use.
--  - Uses Heartbeat and coroutines for stable timing and mobile compatibility.
--  - Be careful where you run this (respect server rules & other players).

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

-- Config
local TELEPORT_OFFSET = 3       -- behind target
local HIT_STEP = 1.5            -- how close to step forward for a 'hit'
local HIT_DELAY = 0.12          -- seconds between hit cycles
local SMOOTH_TIME = 0.06        -- smoothing step time for lerp movements
local GUI_NAME = "PlayerTeleportAndAttackGUI"

-- State
local TargetPlayer = nil
local IsAttacking = false
local AttackThread = nil
local GuiOpen = false
local toggleDebounce = false

-- Utility
local function safeFindHRP(player)
    if not player or not player.Character then return nil end
    return player.Character:FindFirstChild("HumanoidRootPart")
end

local function stopAttack()
    IsAttacking = false
    TargetPlayer = nil
    if AttackThread and coroutine.status(AttackThread) ~= "dead" then
        -- Attack coroutine will exit when IsAttacking is false
    end
end

local function smoothMoveTo(hrp, targetCFrame, duration)
    -- small smooth movement using Heartbeat
    if not hrp or not hrp.Parent then return end
    local start = hrp.CFrame
    local elapsed = 0
    while elapsed < duration do
        if not hrp.Parent then return end
        local dt = RunService.Heartbeat:Wait()
        elapsed = elapsed + dt
        local alpha = math.clamp(elapsed / duration, 0, 1)
        hrp.CFrame = start:Lerp(targetCFrame, alpha)
    end
    -- Ensure final
    if hrp.Parent then hrp.CFrame = targetCFrame end
end

local function attackLoop()
    -- runs in a coroutine
    local localHRP = safeFindHRP(LocalPlayer)
    if not localHRP then warn("Local HRP missing, abort attack") stopAttack() return end

    while IsAttacking do
        if not TargetPlayer then
            stopAttack()
            break
        end

        local targetHRP = safeFindHRP(TargetPlayer)
        local localHRP = safeFindHRP(LocalPlayer)
        if not targetHRP or not localHRP then
            -- target died/left or local died
            stopAttack()
            break
        end

        -- Compute behind position
        local look = targetHRP.CFrame.LookVector
        local behindPos = targetHRP.Position - look * TELEPORT_OFFSET
        behindPos = Vector3.new(behindPos.X, localHRP.Position.Y, behindPos.Z)
        local behindCFrame = CFrame.new(behindPos, targetHRP.Position)

        -- Smooth teleport behind target
        pcall(function() smoothMoveTo(localHRP, behindCFrame, SMOOTH_TIME) end)

        -- Make a small step forward to "hit"
        if not localHRP.Parent or not targetHRP.Parent then break end
        local forwardCFrame = localHRP.CFrame * CFrame.new(0, 0, -HIT_STEP)
        pcall(function() smoothMoveTo(localHRP, forwardCFrame, SMOOTH_TIME) end)

        -- Optional tiny pause to simulate contact
        task.wait(0.02)

        -- Move back to behind position
        if not localHRP.Parent then break end
        pcall(function() smoothMoveTo(localHRP, behindCFrame, SMOOTH_TIME) end)

        -- Wait before next hit cycle
        local elapsed = 0
        while elapsed < HIT_DELAY do
            if not IsAttacking then break end
            local dt = RunService.Heartbeat:Wait()
            elapsed = elapsed + dt
        end
    end
    -- cleanup
    IsAttacking = false
    AttackThread = nil
end

-- Create mobile GUI
local function CreatePlayerGUI()
    -- destroy existing
    if PlayerGui:FindFirstChild(GUI_NAME) then
        PlayerGui[GUI_NAME]:Destroy()
    end

    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.Name = GUI_NAME
    ScreenGui.ResetOnSpawn = false
    ScreenGui.Parent = PlayerGui

    local Frame = Instance.new("Frame")
    Frame.Size = UDim2.new(0, 240, 0, 360)
    Frame.Position = UDim2.new(0.02, 0, 0.08, 0)
    Frame.BackgroundColor3 = Color3.fromRGB(30,30,30)
    Frame.BorderSizePixel = 0
    Frame.Parent = ScreenGui
    Frame.Active = true

    -- Make draggable (mobile-friendly)
    local dragging, dragStart, startPos = false, nil, nil
    Frame.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = Frame.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    Frame.InputChanged:Connect(function(input)
        if dragging and dragStart and startPos then
            local delta = input.Position - dragStart
            Frame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)

    local Title = Instance.new("TextLabel")
    Title.Size = UDim2.new(1, 0, 0, 36)
    Title.Position = UDim2.new(0, 0, 0, 0)
    Title.BackgroundColor3 = Color3.fromRGB(45,45,45)
    Title.TextColor3 = Color3.fromRGB(255,255,255)
    Title.Font = Enum.Font.GothamBold
    Title.TextSize = 18
    Title.Text = "Players"
    Title.Parent = Frame

    local CloseBtn = Instance.new("TextButton")
    CloseBtn.Size = UDim2.new(0, 36, 0, 36)
    CloseBtn.Position = UDim2.new(1, -38, 0, 0)
    CloseBtn.BackgroundTransparency = 1
    CloseBtn.Text = ""
    CloseBtn.Font = Enum.Font.GothamBold
    CloseBtn.TextSize = 20
    CloseBtn.TextColor3 = Color3.fromRGB(220,220,220)
    CloseBtn.Parent = Frame
    CloseBtn.MouseButton1Click:Connect(function()
        if PlayerGui:FindFirstChild(GUI_NAME) then
            PlayerGui[GUI_NAME]:Destroy()
            stopAttack()
            GuiOpen = false
        end
    end)

    local ScrollingFrame = Instance.new("ScrollingFrame")
    ScrollingFrame.Size = UDim2.new(1, -12, 1, -86)
    ScrollingFrame.Position = UDim2.new(0, 6, 0, 40)
    ScrollingFrame.BackgroundTransparency = 1
    ScrollingFrame.BorderSizePixel = 0
    ScrollingFrame.Parent = Frame
    ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
    ScrollingFrame.ScrollBarThickness = 8

    local UIList = Instance.new("UIListLayout")
    UIList.Padding = UDim.new(0, 6)
    UIList.SortOrder = Enum.SortOrder.LayoutOrder
    UIList.Parent = ScrollingFrame

    -- stop/target info area
    local BottomBar = Instance.new("Frame")
    BottomBar.Size = UDim2.new(1, 0, 0, 40)
    BottomBar.Position = UDim2.new(0, 0, 1, -40)
    BottomBar.BackgroundColor3 = Color3.fromRGB(38,38,38)
    BottomBar.BorderSizePixel = 0
    BottomBar.Parent = Frame

    local TargetLabel = Instance.new("TextLabel")
    TargetLabel.Size = UDim2.new(0.7, -8, 1, 0)
    TargetLabel.Position = UDim2.new(0, 8, 0, 0)
    TargetLabel.BackgroundTransparency = 1
    TargetLabel.TextColor3 = Color3.fromRGB(200,200,200)
    TargetLabel.Font = Enum.Font.Gotham
    TargetLabel.TextSize = 14
    TargetLabel.Text = "No target"
    TargetLabel.TextXAlignment = Enum.TextXAlignment.Left
    TargetLabel.Parent = BottomBar

    local StopBtn = Instance.new("TextButton")
    StopBtn.Size = UDim2.new(0.28, -12, 0.8, 0)
    StopBtn.Position = UDim2.new(0.72, 0, 0.1, 0)
    StopBtn.BackgroundColor3 = Color3.fromRGB(70,40,40)
    StopBtn.TextColor3 = Color3.fromRGB(255,255,255)
    StopBtn.Font = Enum.Font.GothamBold
    StopBtn.TextSize = 14
    StopBtn.Text = "Stop"
    StopBtn.Parent = BottomBar
    StopBtn.MouseButton1Click:Connect(function()
        stopAttack()
        TargetLabel.Text = "No target"
    end)

    -- Function to update canvas size when players are added
    local function updateCanvas()
        ScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, UIList.AbsoluteContentSize.Y + 10)
    end
    UIList:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(updateCanvas)

    -- Add player button creator
    local function AddPlayerButton(player)
        if player == LocalPlayer then return end
        -- avoid duplicates
        for _, child in ipairs(ScrollingFrame:GetChildren()) do
            if child:IsA("TextButton") and child.Name == "btn_"..player.UserId then return end
        end

        local btn = Instance.new("TextButton")
        btn.Name = "btn_"..player.UserId
        btn.Size = UDim2.new(1, -12, 0, 44)
        btn.BackgroundColor3 = Color3.fromRGB(60,60,60)
        btn.TextColor3 = Color3.fromRGB(255,255,255)
        btn.Font = Enum.Font.GothamSemibold
        btn.TextSize = 16
        btn.Text = player.Name
        btn.Parent = ScrollingFrame

        btn.MouseButton1Click:Connect(function()
            -- set target and start attack
            if not player or not player.Character then
                warn("Target missing character")
                return
            end
            local targetHRP = safeFindHRP(player)
            local localHRP = safeFindHRP(LocalPlayer)
            if not targetHRP or not localHRP then
                warn("Missing HRP. Wait until both characters exist.")
                return
            end

            -- cleanup previous
            stopAttack()
            TargetPlayer = player
            TargetLabel.Text = "Target: "..player.Name
            IsAttacking = true
            -- start attack coroutine
            if not AttackThread then
                AttackThread = coroutine.create(attackLoop)
                coroutine.resume(AttackThread)
            end
        end)
    end

    -- populate existing players
    for _, pl in ipairs(Players:GetPlayers()) do
        AddPlayerButton(pl)
    end
    -- listen for joins
    Players.PlayerAdded:Connect(AddPlayerButton)

    -- when a player leaves, if it was target, stop
    Players.PlayerRemoving:Connect(function(pl)
        if TargetPlayer == pl then
            stopAttack()
            TargetLabel.Text = "No target"
        end
    end)
end

-- GUI Toggle: mobile floating button
local function createToggleButton()
    if PlayerGui:FindFirstChild("TeleportAttackToggle") then return end

    local sg = Instance.new("ScreenGui")
    sg.Name = "TeleportAttackToggle"
    sg.ResetOnSpawn = false
    sg.Parent = PlayerGui

    local btn = Instance.new("TextButton")
    btn.Name = "ToggleBtn"
    btn.Size = UDim2.new(0, 64, 0, 64)
    btn.Position = UDim2.new(0.9, 0, 0.75, 0)
    btn.AnchorPoint = Vector2.new(0.5, 0.5)
    btn.Text = "TP"
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 18
    btn.Parent = sg
    btn.BackgroundColor3 = Color3.fromRGB(60,60,60)
    btn.TextColor3 = Color3.fromRGB(255,255,255)
    btn.Modal = false
    btn.Active = true

    -- allow drag for mobile
    local dragging, dragStart, startPos = false, nil, nil
    btn.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
            dragging = true
            dragStart = input.Position
            startPos = btn.Position
            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    btn.InputChanged:Connect(function(input)
        if dragging and dragStart and startPos then
            local delta = input.Position - dragStart
            btn.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
        end
    end)

    btn.MouseButton1Click:Connect(function()
        if toggleDebounce then return end
        toggleDebounce = true
        if PlayerGui:FindFirstChild(GUI_NAME) then
            PlayerGui[GUI_NAME]:Destroy()
            stopAttack()
            GuiOpen = false
        else
            CreatePlayerGUI()
            GuiOpen = true
        end
        task.delay(0.25, function() toggleDebounce = false end)
    end)
end

-- Respawn handling
LocalPlayer.CharacterAdded:Connect(function()
    -- when player respawns, stop attack to avoid invalid HRP refs
    stopAttack()
end)

-- Init
createToggleButton()
print("Mobile Player Teleport & Attack GUI loaded. Use the TP button to open the menu.")

    end
